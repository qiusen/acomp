<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dwr PUBLIC "-//GetAhead Limited//DTD Direct Web Remoting 2.0//EN" "http://getahead.org/dwr/dwr20.dtd">
<dwr>
	<allow>
	    <create creator="spring" javascript="catalogDwr">
	      <param name="beanName" value="catalogAjax"/>
	    </create>
	    <create creator="spring" javascript="articleColumnDwr">
	      <param name="beanName" value="articleColumnAjax"/>
	    </create>
	    <create creator="spring" javascript="articleCategoryDwr">
	      <param name="beanName" value="articleCategoryAjax"/>
	    </create>
	    
	    <convert converter="bean" match="com.dihaiboyun.cms.model.Catalog"/>
	    <convert converter="bean" match="com.dihaiboyun.cms.model.ArticleColumn"/>
	    <convert converter="bean" match="com.dihaiboyun.cms.model.ArticleCategory"/>
    
	</allow>
</dwr>