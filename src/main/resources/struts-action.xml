	<!-- 资源Action Begin -->
	<package name="struts-admin-resources" namespace="/admin/resources"
		extends="struts-interceptor">
		<action name="resourcesAction" class="resourcesAction">
			<result name="success" type="dispatcher">/jsp/admin/resources/resourcesList.jsp
			</result>
			<result name="add" type="dispatcher">/jsp/admin/resources/resourcesAdd.jsp
			</result>
			<result name="addSave" type="redirectAction">
				<param name="actionName">resourcesAction</param>
				<param name="namespace">/admin/resources</param>
			</result>

			<result name="edit" type="dispatcher">/jsp/admin/resources/resourcesEdit.jsp
			</result>
			<result name="editSave" type="redirectAction">
				<param name="actionName">resourcesAction</param>
				<param name="namespace">/admin/resources</param>
			</result>

			<result name="deleteSuccess" type="redirectAction">
				<param name="actionName">resourcesAction</param>
				<param name="namespace">/admin/resources</param>
			</result>

			<interceptor-ref name="cms-admin" />
