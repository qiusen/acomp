    <!-- 块管理Action Begin -->
    <package name="struts-admin-blocks" namespace="/admin/blocks" extends="struts-interceptor">
		<action name="blocksAction" class="blocksAction">
			<result name="success" type="dispatcher">/jsp/admin/blocks/blocksList.jsp</result>
			<result name="add" type="dispatcher">/jsp/admin/blocks/blocksAdd.jsp</result>
			<result name="addSave" type="redirectAction">
				<param name="actionName">blocksAction</param>
				<param name="namespace">/admin/blocks</param>
			</result>
			<result name="edit" type="dispatcher">/jsp/admin/blocks/blocksEdit.jsp</result>
			<result name="editSave" type="redirectAction">
				<param name="actionName">blocksAction</param>
				<param name="namespace">/admin/blocks</param>
			</result>
			<result name="deleteSuccess" type="redirectAction">
				<param name="actionName">blocksAction</param>
				<param name="namespace">/admin/blocks</param>
			</result>
			<interceptor-ref name="cms-admin"/>
        </action>
    </package>
	<!-- 块管理Action End -->
    <!-- 栏目管理Action Begin -->
    <package name="struts-admin-catalogs" namespace="/admin/catalogs" extends="struts-interceptor">
		<action name="catalogsAction" class="catalogsAction">
			<result name="success" type="dispatcher">/jsp/admin/catalogs/catalogsTree.jsp</result>
			<result name="preview" type="dispatcher">/jsp/admin/catalogs/catalogsPreview.jsp</result>
			<result name="contentEdit" type="dispatcher">/jsp/admin/catalogs/catalogsContentEdit.jsp</result>
			<result name="contentEditEdit" type="dispatcher">/jsp/admin/catalogs/catalogsContentEditEdit.jsp</result>
			<result name="addSave" type="redirectAction">
				<param name="actionName">catalogsAction?nodeId=${nodeId}</param>
				<param name="namespace">/admin/catalogs</param>
			</result>
			<result name="editSave" type="redirectAction">
				<param name="actionName">catalogsAction?nodeId=${nodeId}</param>
				<param name="namespace">/admin/catalogs</param>
			</result>
			<result name="deleteSuccess" type="redirectAction">
				<param name="actionName">catalogsAction?nodeId=${nodeId}</param>
				<param name="namespace">/admin/catalogs</param>
			</result>
			<interceptor-ref name="cms-admin"/>
        </action>
    </package> 
    <!-- 栏目管理Action End -->
	<!-- 模板管理Action Begin -->
    <package name="struts-admin-templates" namespace="/admin/templates" extends="struts-interceptor">
		<action name="templatesAction" class="templatesAction">
			<result name="success" type="dispatcher">/jsp/admin/templates/templatesList.jsp</result>
			<result name="add" type="dispatcher">/jsp/admin/templates/templatesAdd.jsp</result>
			<result name="addSave" type="redirectAction">
				<param name="actionName">templatesAction</param>
				<param name="namespace">/admin/templates</param>
			</result>

			<result name="edit" type="dispatcher">/jsp/admin/templates/templatesEdit.jsp</result>
			<result name="editSave" type="redirectAction">
				<param name="actionName">templatesAction</param>
				<param name="namespace">/admin/templates</param>
			</result>
			
			<result name="deleteSuccess" type="redirectAction">
				<param name="actionName">templatesAction</param>
				<param name="namespace">/admin/templates</param>
			</result>
			<interceptor-ref name="cms-admin"/>
        </action>
    </package> 
