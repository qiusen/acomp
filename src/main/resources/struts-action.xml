	<!-- 磁盘文件Action Begin -->
	<package name="struts-admin-diskFile" namespace="/admin/diskFile"
		extends="struts-interceptor">
		<action name="diskFileAction" class="diskFileAction">
			<result name="success" type="dispatcher">/jsp/admin/diskFile/diskFileList.jsp
			</result>
			<result name="refresh" type="dispatcher">/jsp/admin/diskFile/diskFileRefresh.jsp
			</result>
			<result name="refreshSave" type="dispatcher">/jsp/admin/diskFile/diskFileRefreshSave.jsp
			</result>
			<interceptor-ref name="cms-admin" />
		</action>
	</package>
