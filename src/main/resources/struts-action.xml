    <!-- 角色Action Begin -->
    <package name="struts-admin-roles" namespace="/admin/roles" extends="struts-interceptor">
		<action name="rolesAction" class="rolesAction">
			<result name="success" type="dispatcher">/jsp/admin/roles/rolesList.jsp</result>
			<result name="add" type="dispatcher">/jsp/admin/roles/rolesAdd.jsp</result>
			<result name="addSave" type="redirectAction">
				<param name="actionName">rolesAction</param>
				<param name="namespace">/admin/roles</param>
			</result>

			<result name="edit" type="dispatcher">/jsp/admin/roles/rolesEdit.jsp</result>
			<result name="editSave" type="redirectAction">
				<param name="actionName">rolesAction</param>
				<param name="namespace">/admin/roles</param>
			</result>
			
			<result name="deleteSuccess" type="redirectAction">
				<param name="actionName">rolesAction</param>
				<param name="namespace">/admin/roles</param>
			</result>
			
			<result name="save" type="redirectAction">
				<param name="actionName">catalogsAction</param>
				<param name="namespace">/admin/catalogs</param>
			</result>
			
			<interceptor-ref name="cms-admin"/>
        </action>
    </package> 
