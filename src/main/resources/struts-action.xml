	<!-- 资源Action End -->
    <!-- 资源动作Action Begin -->
    <package name="struts-admin-resourceAction" namespace="/admin/resourceAction" extends="struts-interceptor">
		<action name="resourceActionAction" class="resourceActionAction">
			<result name="success" type="dispatcher">/jsp/admin/resourceAction/resourceActionList.jsp</result>
			<result name="add" type="dispatcher">/jsp/admin/resourceAction/resourceActionAdd.jsp</result>
			<result name="addSave" type="redirectAction">
				<param name="actionName">resourceActionAction</param>
				<param name="namespace">/admin/resourceAction</param>
			</result>

			<result name="edit" type="dispatcher">/jsp/admin/resourceAction/resourceActionEdit.jsp</result>
			<result name="editSave" type="redirectAction">
				<param name="actionName">resourceActionAction</param>
				<param name="namespace">/admin/resourceAction</param>
			</result>
			
			<result name="deleteSuccess" type="redirectAction">
				<param name="actionName">resourceActionAction</param>
				<param name="namespace">/admin/resourceAction</param>
			</result>
			
			<interceptor-ref name="cms-admin"/>
