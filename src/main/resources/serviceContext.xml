<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
	<!-- 管理员 service -->	
	<bean id="managerService" class="com.dihaitech.acomp.service.impl.ManagerServiceImpl"/>
	
	<!-- 角色 service -->	
	<bean id="roleService" class="com.dihaitech.acomp.service.impl.RoleServiceImpl"/>
	
	<!-- 菜单 service -->	
	  <bean id="menuService" class="com.dihaitech.acomp.service.impl.MenuServiceImpl"/>
	
	<!-- 目录 service -->	
	  <bean id="catalogService" class="com.dihaitech.acomp.service.impl.CatalogServiceImpl"/>
	
	<!-- 模块 service -->	
	  <bean id="moduleService" class="com.dihaitech.acomp.service.impl.ModuleServiceImpl"/>
	  
	<!-- 市 service -->	
	  <bean id="cityService" class="com.dihaitech.acomp.service.impl.CityServiceImpl"/>
	  
	<!-- 省 service -->	
	  <bean id="provinceService" class="com.dihaitech.acomp.service.impl.ProvinceServiceImpl"/>
	  
	<!-- 区 service -->	
	  <bean id="areaService" class="com.dihaitech.acomp.service.impl.AreaServiceImpl"/>
	  
