	  <bean id="articleService" class="com.dihaitech.acomp.service.impl.ArticleServiceImpl"/>
	  
	  <!-- 块 service -->	
	  <bean id="blockService" class="com.dihaitech.acomp.service.impl.BlockServiceImpl"/>
	  
